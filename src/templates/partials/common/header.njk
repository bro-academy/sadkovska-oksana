<header class="header">
  <a class="header__link logo" href="/">
    <img class="header__logo logo_img" src="img/logo.svg" alt="Logo"/>
    <img src="img/logo-heading.svg" class="logo__heading" alt="Logo"></img>
  </a>
  <nav class="header__navigation">
    <ul class="header__list">

      <li class="header__element">
        <a class="header__link-menu header__link--active" href="#">{{ header_nav_underlined }}</a>
      </li>
      {% for item in header_nav %}
        <li class="header__element">
        <a class="header__link-menu" href="#">{{ item.element }}</a>
        </li>
      {% endfor %}
     
    </ul>
  </nav>
  <div class="header__search">
    <div class="search divider-vertical divider-vertical--right">
      <img class="search__icon" src="img/search.svg" alt="Search"/>
      <input class="search__input" type="search" placeholder="Search"/>
    </div>

  </div>

  <div class="header__btn">
    <a class="button button--secondary" href="#" target="_blank">
      <img src="img/discord.svg" class="button__icon button__icon--left" alt="discort"/>
     {{ header_btn_secondary }}
      </a>
    <button class="button button--primary">
      <img src="img/wallet.svg" class="button__icon button__icon--left" alt="wallet"/>
       {{ header_btn_primary }}</button>
  </div>

</header>